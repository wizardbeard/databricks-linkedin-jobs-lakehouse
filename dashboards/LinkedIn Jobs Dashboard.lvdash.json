      "schema": "portfolio_jobs"
    },
    {
      "name": "8882bd4b",
      "displayName": "LinkedIn Top Companies",
      "queryLines": [
        "SELECT * FROM workspace.portfolio_jobs.v_gold_top_companies;"
      ]
    }
  ],
  "pages": [
    {
      "name": "0cdc5036",
      "displayName": "LinkedIn Posts by Company",
      "layout": [
        {
          "widget": {
            "name": "job-posts-by-date",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "15945b87",
                  "fields": [
                    {
                      "name": "posted_date",
                      "expression": "`posted_date`"
                    },
                    {
                      "name": "job_posts",
                      "expression": "`job_posts`"
                    }
                  ],
                  "disaggregated": true
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "job_posts"
                },
                "target": {
                  "fieldName": "posted_date"
                }
              },
              "frame": {
                "showTitle": true,
                "title": "Total Job Posts by Date",
                "showDescription": true,
                "description": "Count the number of quality job posts"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 6,
            "height": 6
          }
        },
        {
          "widget": {
            "name": "job-posts-by1",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "8882bd4b",
                  "fields": [
                    {
                      "name": "job_posts",
                      "expression": "`job_posts`"
                    },
                    {
                      "name": "company",
                      "expression": "`company`"
                    }
                  ],
                  "disaggregated": true
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "job_posts",
                  "displayName": "Job posts",
                  "scale": {
                    "type": "quantitative"
                  }
                },
                "y": {
                  "fieldName": "company",
                  "displayName": "Company",
                  "scale": {
                    "type": "categorical"
                  }
                },
                "label": {
                  "show": true
                }
              },
              "frame": {
                "showTitle": true,
                "title": "Job Posts from the Top 20 Companies"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 6,
            "width": 6,
            "height": 6
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS"
